[general]
state_file = /var/awslogs/state/agent-state

[/var/log/syslog]
file = /var/log/syslog
log_group_name = /var/log/syslog
log_stream_name = {instance_id}
datetime_format = %b %d %H:%M:%S

[/opt/kafka/log/kafka/server.log]
file = /opt/kafka/log/kafka/server.log*
log_group_name = opt/kafka/log/kafka/server.log
log_stream_name = {instance_id}
datetime_format = %d %b %Y %H:%M:%S
